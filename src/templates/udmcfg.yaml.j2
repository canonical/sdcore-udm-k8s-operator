configuration:
  keys:
    udmProfileAHNPrivateKey: {{ _home_network_private_key }}
  nrfUri: {{ nrf_url }}
  webuiUri: {{ webui_uri }}
  sbi:
    bindingIPv4: 0.0.0.0
    port: {{ udm_sbi_port }}
    registerIPv4: {{ pod_ip }}
    scheme: {{ scheme }}
    tls:
      key: {{ tls_key }}
      pem: {{ tls_pem }}
  enableNrfCaching: true
  nrfCacheEvictionInterval: 900
  serviceList:
  - nudm-sdm
  - nudm-uecm
  - nudm-ueau
  - nudm-ee
  - nudm-pp
info:
  description: UDM initial local configuration
  version: 1.0.0
logger:
  UDM:
    debugLevel: {{ log_level }}
